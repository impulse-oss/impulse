{
  "name": "@impulse.dev/runtime",
  "version": "0.1.4",
  "description": "yes-code UI editor",
  "homepage": "https://impulse.dev",
  "keywords": [
    "react",
    "tailwind",
    "nocode"
  ],
  "license": "MIT",
  "repository": "github:impulse-oss/impulse",
  "files": [
    "src",
    "dist",
    "inject.js",
    "README.md"
  ],
  "main": "./dist/impulse.umd.js",
  "types": "./dist/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/impulse.es.js",
      "require": "./dist/impulse.umd.js",
      "type": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "lint": "eslint \"./**/*.{ts,tsx,js}\" --fix",
    "build": "vite build",
    "clean": "rm -rf dist",
    "dev": "npm run clean && concurrently npm:dev:*",
    "dev:lib": "vite build --watch --outDir dist",
    "dev:app": "cd dev-app && vite --port 3005",
    "publish-stable": "npm publish --access public && curl https://purge.jsdelivr.net/npm/@impulse.dev/runtime@latest"
  },
  "devDependencies": {
    "@babel/standalone": "^7.18.5",
    "@babel/types": "^7.18.4",
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@headlessui/react": "^1.6.4",
    "@mui/material": "^5.8.3",
    "@types/babel__core": "^7.1.19",
    "@types/babel__standalone": "^7.1.4",
    "@types/babel__traverse": "^7.17.1",
    "@types/node": "^17.0.42",
    "@types/path-browserify": "^1.0.0",
    "@types/prettier": "^2.6.3",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "@types/wicg-file-system-access": "^2020.9.5",
    "@vitejs/plugin-react": "^1.3.2",
    "animated-scroll-to": "^2.2.0",
    "autoprefixer": "^10.4.7",
    "click-to-react-component": "^1.0.8",
    "concurrently": "^7.2.1",
    "idb-keyval": "^6.1.0",
    "kbar": "^0.1.0-beta.35",
    "path-browserify": "^1.0.1",
    "postcss": "^8.4.14",
    "postcss-nested": "^5.0.6",
    "prettier": "^2.6.2",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "rxjs": "^7.5.5",
    "tailwindcss": "^3.1.2",
    "tsup": "^6.1.2",
    "typescript": "^4.7.3",
    "vite": "^2.9.12",
    "vite-plugin-dts": "^1.2.0"
  }
}
