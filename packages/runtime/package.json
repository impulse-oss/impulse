{
  "name": "@impulse.dev/runtime",
  "version": "0.1.5",
  "description": "yes-code UI editor",
  "homepage": "https://impulse.dev",
  "keywords": [
    "react",
    "tailwind",
    "nocode"
  ],
  "license": "MIT",
  "repository": "github:impulse-oss/impulse",
  "files": [
    "src",
    "dist",
    "inject.js",
    "README.md"
  ],
  "main": "./dist/impulse.umd.js",
  "types": "./dist/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/impulse.es.js",
      "require": "./dist/impulse.umd.js",
      "type": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "lint": "eslint \"./**/*.{ts,tsx,js}\" --fix",
    "build": "vite build",
    "clean": "rm -rf dist",
    "dev": "npm run clean && concurrently npm:dev:*",
    "dev:lib": "vite build --watch",
    "dev:app": "cd dev-app && vite",
    "reset-cdn-cache": "curl https://purge.jsdelivr.net/npm/@impulse.dev/runtime@latest && curl https://cdn.jsdelivr.net/npm/@impulse.dev/runtime@latest",
    "publish-stable": "npm publish --access public && npm run reset-cdn-cache"
  },
  "devDependencies": {
    "@babel/types": "^7.18.9",
    "@codemod/core": "^2.0.1",
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@headlessui/react": "^1.6.6",
    "@mui/material": "^5.9.2",
    "@types/babel__core": "^7.1.19",
    "@types/babel__standalone": "^7.1.4",
    "@types/babel__traverse": "^7.17.1",
    "@types/node": "^18.6.1",
    "@types/path-browserify": "^1.0.0",
    "@types/prettier": "^2.6.4",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/wicg-file-system-access": "^2020.9.5",
    "@vitejs/plugin-react": "^2.0.0",
    "animated-scroll-to": "^2.3.0",
    "assert": "^2.0.0",
    "autoprefixer": "^10.4.7",
    "click-to-react-component": "^1.0.8",
    "concurrently": "^7.3.0",
    "idb-keyval": "^6.2.0",
    "kbar": "^0.1.0-beta.36",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "postcss": "^8.4.14",
    "postcss-nested": "^5.0.6",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rxjs": "^7.5.6",
    "tailwindcss": "^3.1.6",
    "tsup": "^6.2.0",
    "typescript": "^4.7.4",
    "vite": "^3.0.3",
    "vite-plugin-dts": "^1.4.0"
  }
}
